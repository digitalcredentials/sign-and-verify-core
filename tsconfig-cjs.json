    /* 
    Our typescript compiler options to generate commonJS
    Overrides only the compiler options from tsconfig.json
    */
    
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "outDir": "./dist/cjs",
    "typeRoots": [
      "./types",
      "./node_modules/@types"
    ]
  }
}